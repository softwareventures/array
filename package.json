  },
  "sideEffects": false,
  "devDependencies": {
    "@softwareventures/tsconfig": "2.2.0",
    "@softwareventures/tslint-rules": "0.9.1",
    "ava": "3.9.0",
    "cz-conventional-changelog": "3.2.0",
    "semantic-release": "17.1.1",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.9.6"
  },
  "dependencies": {
    "@softwareventures/dictionary": "^0.6.0",
    "@softwareventures/ordered": "^0.2.0",
    "dictionary-types": "^1.2.0",
    "tslib": "^1.9.3 || ^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
